	pages = {293--302},
	file = {PubMed Central Full Text PDF:C\:\\Users\\21209979\\Zotero\\storage\\UJ8Z9V38\\Betancourt et al. - 2003 - Defining cultural competence a practical framewor.pdf:application/pdf}
}

