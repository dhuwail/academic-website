	pages = {475--480},
	file = {Full Text PDF:C\:\\Users\\21209979\\Zotero\\storage\\9VEJVNDH\\Mant - 2001 - Process versus outcome indicators in the assessmen.pdf:application/pdf;Int J Qual Health Care-2001-Mant-475-80.pdf:C\:\\Users\\21209979\\Zotero\\storage\\M5FKT7IT\\Int J Qual Health Care-2001-Mant-475-80.pdf:application/pdf;Snapshot:C\:\\Users\\21209979\\Zotero\\storage\\H9BD3PSB\\475.html:text/html}
