	number = {8 Suppl 3},
	journal = {Medical Care},
	author = {Hersh, William R. and Weiner, Mark G. and Embi, Peter J. and Logan, Judith R. and Payne, Philip R. O. and Bernstam, Elmer V. and Lehmann, Harold P. and Hripcsak, George and Hartzog, Timothy H. and Cimino, James J. and Saltz, Joel H.},
