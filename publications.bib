@article{el_emam_evaluating_2009-1,
	title = {Evaluating predictors of geographic area population size cut-offs to manage re-identification risk},
	volume = {16},
	doi = {10.1197/jamia.M2902},
	abstract = {OBJECTIVE: In public health and health services research, the inclusion of geographic information in data sets is critical. Because of concerns over the re-identification of patients, data from small geographic areas are either suppressed or the geographic areas are aggregated into larger ones. Our objective is to estimate the population size cut-off at which a geographic area is sufficiently large so that no data suppression or further aggregation is necessary. DESIGN: The 2001 Canadian census data were used to conduct a simulation to model the relationship between geographic area population size and uniqueness for some common demographic variables. Cut-offs were computed for geographic area population size, and prediction models were developed to estimate the appropriate cut-offs. MEASUREMENTS: Re-identification risk was measured using uniqueness. Geographic area population size cut-offs were estimated using the maximum number of possible values in the data set and a traditional entropy measure. RESULTS: The model that predicted population cut-offs using the maximum number of possible values in the data set had R2 values around 0.9, and relative error of prediction less than 0.02 across all regions of Canada. The models were then applied to assess the appropriate geographic area size for the prescription records provided by retail and hospital pharmacies to commercial research and analysis firms. CONCLUSIONS: To manage re-identification risk, the prediction models can be used by public health professionals, health researchers, and research ethics boards to decide when the geographic area population size is sufficiently large.},
	number = {2},
	journal = {Journal of the American Medical Informatics Association},
	author = {El Emam, Khaled and Brown, Ann and AbdelMalik, Philip},
	year = {2009},
	pages = {256--66}
}

