	keywords = {Humans, User-Computer Interface, Child, Attitude to Computers, Algorithms, Asthma, Computer Systems, Emergency Service, Hospital, Medical History Taking, Medical Informatics Applications, Medical Records Systems, Computerized, Parents, Practice Guidelines as Topic, Software},
