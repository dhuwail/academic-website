	keywords = {Humans, Health Care Surveys, United States, Diffusion of Innovation, Attitude of Health Personnel, Electronic Health Records, Physicians, Cross-Sectional Studies, Quality of Health Care, Meaningful Use, Professional Competence},
	pages = {392--404}
}

