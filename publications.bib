	keywords = {Humans, *Video Games, Surveys and Questionnaires, Child, Patient Education as Topic/*methods, *Diabetes Mellitus, Type 1, Blood Glucose, Exercise, diet, Insulin/administration \& dosage},
	pages = {855--859}
}

