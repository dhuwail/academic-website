	file = {ScienceDirect Full Text PDF:C\:\\Users\\21209979\\Zotero\\storage\\CRIMQGXN\\McCann et al. - 2014 - Urban and rural differences in risk of admission t.pdf:application/pdf;ScienceDirect Snapshot:C\:\\Users\\21209979\\Zotero\\storage\\ZI4V7U4A\\S1353829214001397.html:text/html}
}

