	file = {Su and Chiang - 2013 - IAServ An Intelligent Home Care Web Services Plat.pdf:C\:\\Users\\21209979\\Zotero\\storage\\VDJRZFRX\\Su and Chiang - 2013 - IAServ An Intelligent Home Care Web Services Plat.pdf:application/pdf}
