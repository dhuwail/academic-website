	pmid = {21912892},
	keywords = {Adolescent, Female, Humans, Male, Surveys and Questionnaires, Child, Risk Factors, Case-Control Studies, Logistic Models, Socioeconomic Factors, sleep, Body Mass Index, Prevalence, Health Surveys, China/epidemiology, diet, Obesity/*epidemiology/etiology, Sedentary Lifestyle},
