	urldate = {2015-11-18},
	journal = {JAMA : the journal of the American Medical Association},
	author = {Dykes, Patricia C. and Carroll, Diane L. and Hurley, Ann and Lipsitz, Stuart and Benoit, Angela and Chang, Frank and Meltzer, Seth and Tsurikova, Ruslana and Zuyov, Lyubov and Middleton, Blackford},
	month = nov,
	year = {2010},
	pmid = {21045097},
	pmcid = {PMC3107709},
	pages = {1912--1918},
	file = {Dykes et al. - 2010 - Fall Prevention in Acute Care Hospitals.pdf:C\:\\Users\\21209979\\Zotero\\storage\\WDE95MX4\\Dykes et al. - 2010 - Fall Prevention in Acute Care Hospitals.pdf:application/pdf}
}

