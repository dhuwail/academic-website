	urldate = {2015-12-09},
	journal = {The American Journal of Surgery},
	author = {Siracuse, Jeffrey J. and Odell, David D. and Gondek, Stephen P. and Odom, Stephen R. and Kasper, Ekkehard M. and Hauser, Carl J. and Moorman, Donald W.},
	month = mar,
