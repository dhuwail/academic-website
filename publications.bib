	pmid = {22480067},
	keywords = {Adolescent, Female, Humans, Male, Child, Health Behavior, Motor Activity, *Sedentary Lifestyle, *Diabetes Mellitus, Type 1/blood/psychology/therapy, Hemoglobin A, Glycosylated/analysis, Italy/epidemiology},
	pages = {124--131}
}

