@article{sidorov_it_2006,
	title = {It {Ain}’t {Necessarily} {So}: {The} {Electronic} {Health} {Record} {And} {The} {Unlikely} {Prospect} {Of} {Reducing} {Health} {Care} {Costs}},
	volume = {25},
