@article{crist_cafecitos_2005,
	title = {Cafecitos and {Telenovelas}: {Culturally} {Competent} {Interventions} to {Facilitate} {Mexican} {American} {Families}’ {Decisions} to {Use} {Home} {Care} {Services}},
