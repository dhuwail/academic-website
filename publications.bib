	journal = {International Journal of Nursing Studies},
	author = {Goodridge, Donna and Hawranik, Pamela and Duncan, Vicky and Turner, Hollie},
	month = oct,
	year = {2012},
	keywords = {home health care, Disparities, Community-based care, Equity, Socioeconomic status},
	pages = {1310--1319},
	file = {Goodridge et al. - 2012 - Socioeconomic disparities in home health care serv.pdf:C\:\\Users\\21209979\\Zotero\\storage\\UEEQDMMX\\Goodridge et al. - 2012 - Socioeconomic disparities in home health care serv.pdf:application/pdf;ScienceDirect Snapshot:C\:\\Users\\21209979\\Zotero\\storage\\UNRSJW6G\\S002074891200003X.html:text/html}
