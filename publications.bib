}

@article{phan-hug_impact_2011-1,
	title = {Impact of videogame playing on glucose metabolism in children with type 1 diabetes.},
	volume = {12},
	copyright = {(c) 2011 John Wiley \& Sons A/S.},
	issn = {1399-5448 1399-543X},
	doi = {10.1111/j.1399-5448.2011.00770.x},
	abstract = {Time spent playing videogames (VG) occupies a continually increasing part of children's leisure time. They can generate an important state of excitation, representing a form of mental and physical stress. This pilot study aimed to assess whether VG influences glycemic balance in children with type 1 diabetes. Twelve children with type 1 diabetes were subjected to two distinct tests at a few weeks interval: (i) a 60-min VG session followed by a 60-min rest period and  (ii) a 60-min reading session followed by a 60-min rest period. Heart rate, blood pressure, glycemia, epinephrine (E), norepinephrine (NE), cortisol (F), and growth hormone (GH) were measured at 30 min intervals from -60 to +120 min. Non-parametric Wilcoxon tests for paired data were performed on Delta-values computed from baseline (0 min). Rise in heart rate (p = 0.05) and NE increase (p  = 0.03) were shown to be significantly higher during the VG session when compared to the reading session and a significant difference of Delta-glycemic values was  measured between the respective rest periods. This pilot study suggests that VG playing could induce a state of excitation sufficient to activate the sympathetic system and alter the course of glycemia. Dietary and insulin dose recommendations may be needed to better control glycemic excursion in children playing VG.},
	language = {eng},
	number = {8},
	journal = {Pediatric diabetes},
	author = {Phan-Hug, Franziska and Thurneysen, Esther and Theintz, Gerald and Ruffieux, Christiane and Grouzmann, Eric},
	month = dec,
	year = {2011},
	pmid = {21564452},
	keywords = {Adolescent, Female, Humans, Male, *Video Games, PILOT projects, Child, Blood Pressure, Blood Glucose/*metabolism, Diabetes Mellitus, Type 1/*blood, Epinephrine/blood, Human Growth Hormone/blood, Hydrocortisone/blood, Norepinephrine/blood, Reading, Stress, Psychological/*physiopathology},
	pages = {713--717}
}

