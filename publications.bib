	keywords = {Female, Humans, Male, Child, Students, Personal Satisfaction, Video Games/*psychology, Asian Continental Ancestry Group/*psychology, Diabetes Mellitus, Type 2/*prevention \& control, Healthy Diet/*psychology, Hong Kong, Pediatric Obesity/*prevention \& control},
