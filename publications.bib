	keywords = {Humans, *Video Games, Health Behavior, Diabetes Mellitus/blood/*therapy, Efficiency, Endocrinology/*instrumentation/methods/trends, Equipment Design, Self Care/*instrumentation/*methods},
	pages = {802--806}
