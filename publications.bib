@article{deshazo_designing_2010,
	title = {Designing and remotely testing mobile diabetes video games.},
	volume = {16},
	issn = {1758-1109 1357-633X},
	doi = {10.1258/jtt.2010.091012},
	abstract = {We have investigated game design and usability for three mobile phone video games designed to deliver diabetes education. The games were refined using focus groups. Six people with diabetes participated in the first focus group and five in the second. Following the focus groups, we incorporated the new findings into  the game design, and then conducted a field test to evaluate the games in the context in which they would actually be used. Data were collected remotely about  game usage by eight people with diabetes. The testers averaged 45 seconds per question and answered an average of 50 total nutrition questions each. They self-reported playing the game for 10-30 min, which coincided with the measured metrics of the game. Mobile games may represent a promising new way to engage the user and deliver relevant educational content.},
	language = {eng},
	number = {7},
	journal = {Journal of telemedicine and telecare},
	author = {DeShazo, Jonathan and Harris, Lynne and Turner, Anne and Pratt, Wanda},
	year = {2010},
	pmid = {20679406},
	keywords = {Humans, *Video Games, *Cell Phones, *Diabetes Mellitus, Focus Groups, Nutritive Value, Patient Education as Topic/*methods, Patient Satisfaction, Self Care},
	pages = {378--382}
}

