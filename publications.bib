	file = {Basu and Blanning - 2000 - A Formal Approach to Workflow Analysis.pdf:C\:\\Users\\21209979\\Zotero\\storage\\RTN29MV3\\Basu and Blanning - 2000 - A Formal Approach to Workflow Analysis.pdf:application/pdf}
}

